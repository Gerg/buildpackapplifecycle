#!/bin/bash
# vim: set ft=sh

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$4

echo WOO

if [ -e "$CACHE_DIR/old-compile" ]; then
  contents=$(cat "$CACHE_DIR/old-compile")
else
  contents="also-always-detects-buildpack"
fi

echo $contents > $BUILD_DIR/compiled
echo $contents > $CACHE_DIR/compiled
echo $contents > $DEPS_DIR/compiled